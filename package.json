{
  "name": "design-dock",
  "version": "1.0.0",
  "description": "My breathtaking Nuxt.js project",
  "author": "sancyo",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:nuxt": "nuxt",
    "dev:processmd": "processmd content/blog/**/*.md --watch --stdout --outputDir ./content/json > ./content/summary.json",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "md": "processmd content/blog/**/*.md --stdout --outputDir ./content/json > ./content/summary.json"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "nuxt": "^2.0.0",
    "processmd": "^4.5.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "stylelint": "^15.10.1"
  }
}
